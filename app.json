{
  "expo": {
    "name": "UniMap",
    "slug": "UniMap",
    "version": "1.0.0",
    "scheme": "unimap",
    "platforms": ["android", "ios"],
    "web": {
      "favicon": "./assets/favicon.png",
      "s_icon_0": "./assets/avatars/0.png",
      "s_icon_1": "./assets/avatars/1.png",
      "s_icon_2": "./assets/avatars/2.png",
      "s_icon_3": "./assets/avatars/3.png",
      "s_icon_4": "./assets/avatars/4.png",
      "s_icon_5": "./assets/avatars/5.png",
      "s_icon_6": "./assets/avatars/6.png",
      "s_icon_7": "./assets/avatars/7.png",
      "s_icon_8": "./assets/avatars/8.png",
      "s_icon_9": "./assets/avatars/9.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    
    "plugins": [
      "expo-asset",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "sounds": ["./assets/notification.mp3"]
        }
      ]
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.faustyyn.UniMap",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.faustyyn.UniMap"
    },
    "extra": {
      "eas": {
        "projectId": "0299bc91-3bbc-48c8-be5d-4eae4ee7e6dd"
      }
    }
  }
}
